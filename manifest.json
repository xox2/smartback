{
   "manifest_version": 3,
   "name": "Smart Back",
   "version": "2.1.1",
   "description": "Use the back button twice on a tab with no history to close it and return to the previous tab.",
   "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_start"
   } ],
   "permissions": [
      "tabs",
      "storage"
   ],
   "action": {
      "default_title": "Smart Back"
   }
}